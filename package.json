{
  "name": "n8n-nodes-basecamp-complete",
  "version": "1.0.4",
  "description": "n8n community node for Basecamp 4 API integration - manage projects, to-dos, messages, campfires, cards, documents and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "basecamp",
    "workflow",
    "automation",
    "project-management",
    "37signals"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Ammar-Alshuaibi/n8n-nodes-basecamp",
  "author": {
    "name": "Ammar Alshuaibi",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ammar-Alshuaibi/n8n-nodes-basecamp.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BasecampOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Basecamp/Basecamp.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
